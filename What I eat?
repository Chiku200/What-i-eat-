#include<stdio.h>
main(){
printf("Pick up any iteam \n1.pizza \n2.burger \n3.pasta \n4.French fries\n5.Sandwich");
int choice=0;
scanf("%d",&choice);
switch(choice){
case 1:
printf("Food iteam- pizza \n Rs 239");
break;
case 2:
printf("Food iteam- burger \n Rs 129");
break;
case 3:
printf("Food iteam- pasta \n Rs 145");
break;
case 4:
printf("Food iteam- French fire\n Rs 30");
break;
case 5:
printf("Food iteam- Sandwich \n Rs 235");
break;
default:
printf("Invalid choice");
}
}
